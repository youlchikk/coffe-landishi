        <Image HorizontalAlignment="Left" Height="164" Margin="41,51,0,0" VerticalAlignment="Top" Width="188" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
        <Image Margin="41,57,542,122" Source="/Image3.png" Stretch="Fill"/>
        <TextBox HorizontalAlignment="Left" Margin="291,95,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="376" Height="21"/>
        <TextBox HorizontalAlignment="Left" Margin="291,157,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="376" Height="21"/>
        <TextBox HorizontalAlignment="Left" Margin="291,223,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="376" Height="21"/>
        <TextBox HorizontalAlignment="Left" Margin="291,284,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="376" Height="21"/>
        <Label Content="Номер телефона" HorizontalAlignment="Left" Margin="291,116,0,0" VerticalAlignment="Top" Height="48" Width="329" FontFamily="Monotype Corsiva" FontSize="22"/>
        <Label Content="Адрес электронной почты" HorizontalAlignment="Left" Margin="291,183,0,0" VerticalAlignment="Top" Height="48" Width="329" FontFamily="Monotype Corsiva" FontSize="22"/>
        <Label Content="Дата рождения" HorizontalAlignment="Left" Margin="291,249,0,0" VerticalAlignment="Top" Height="48" Width="329" FontFamily="Monotype Corsiva" FontSize="22"/>
        <Label Content="Имя" HorizontalAlignment="Left" Margin="291,56,0,0" VerticalAlignment="Top" Height="47" Width="329" FontFamily="Monotype Corsiva" FontSize="22"/>
    </Grid>
</Window>
